<!-- include Header -->
<%- include('include/_header') %>
<!-- /include Header -->

<!-- Main Section -->
<main id="site-main">
  <div class="container">
    <!-- Main Section heading -->
    <div class="box-nav d-flex">
      <a href="/add-user" class="border-shadow">
        <span class="text-gradient"
          >New User &nbsp;<i class="fa-solid fa-user"></i>
        </span>
      </a>
    </div>

    <!-- Data in tabular format -->
    <form action="/" method="POST">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Gender</th>
            <th>Status</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <%- include('include/_show') %>
        </tbody>
      </table>
    </form>
  </div>
</main>
<!-- Main Section -->

<!-- include footer -->
<%- include('include/_footer') %>
<!-- /include footer -->
